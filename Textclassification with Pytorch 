    {
      "cell_type": "code",
      "source": [
        "def clean_stopwords_shortwords(w):\n",
        "    stopwords_list=stopwords.words('english')\n",
        "    words = w.split() \n",
        "    clean_words = [word for word in words if (word not in stopwords_list) and len(word) > 2]\n",
        "    return \" \".join(clean_words) "
      ],
      "metadata": {
        "id": "YF_6wY1kSutK"
      },
      "execution_count": 9,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "df['cleaned_text']=df['cleaned_text'].map(clean_stopwords_shortwords)"
      ],
      "metadata": {
        "id": "_LMelut_SuqB"
      },
      "execution_count": 10,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "\n",
        "df['labels'] = df['labels'].map({'Checking or savings account':0,'Credit card or prepaid card':1,'Debt collection':2,'Mortgage':3,'Student loan':4,'Vehicle loan or lease':5})\n",
        "\n",
